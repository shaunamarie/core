#######################################################
#
# Test ignore_missing_inputs - control case
#
#######################################################

body common control
{
  inputs => { ".*.cf.sub" };
  expand_input_wildcards => "true";
  ignore_missing_inputs => "true";
  bundlesequence  => { default("$(this.promise_filename)") };
  version => "1.0";
nova_edition::
  host_licenses_paid => "5";
}

#######################################################

bundle agent init
{
vars:
        "dummy" string => "dummy";
}

#######################################################

bundle agent test
{
vars:
        "dummy" string => "dummy";
}

#######################################################

bundle agent check
{
vars:
        "dummy" string => "dummy";

classes:
        "ok" expression => "any";

reports:
    ok::
        "$(this.promise_filename) Pass";
    !ok::
        "$(this.promise_filename) FAIL";
}
